{
	"name": "sveltekit-starter",
	"version": "0.0.1",
	"private": true,
	"prisma": {
		"seed": "tsx prisma/seed.ts"
	},
	"scripts": {
		"generate": "prisma generate",
		"start:dev": "vite dev --port 5169",
		"dev": "pnpm db:push && pnpm db:seed && pnpm run start:dev",
		"build": "pnpm run generate && vite build",
		"db:seed": "prisma db seed",
		"db:push": "prisma db push",
		"docker:up": "docker-compose -p sveltekit-starter up -d",
    	"docker:down": "docker-compose -p sveltekit-starter down --remove-orphans --volumes",
		"start": "node build/index.js",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@faker-js/faker": "^8.4.1",
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-node": "1.0.0-next.106",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/eslint": "^8.56.0",
		"@typescript-eslint/eslint-plugin": "^7.0.0",
		"@typescript-eslint/parser": "^7.0.0",
		"autoprefixer": "^10.4.19",
		"daisyui": "^4.10.2",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0-next.4",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"prisma": "^5.13.0",
		"prisma-zod-generator": "^0.8.13",
		"svelte": "^5.0.0-next.1",
		"svelte-check": "^3.6.0",
		"tailwindcss": "^3.4.3",
		"tslib": "^2.4.1",
		"tsx": "^4.10.0",
		"typescript": "^5.0.0",
		"vite": "^5.0.3"
	},
	"type": "module",
	"dependencies": {
		"@lucia-auth/adapter-prisma": "^4.0.1",
		"@prisma/client": "^5.13.0",
		"@tanstack/svelte-query": "^5.32.0",
		"@trpc/client": "^10.45.2",
		"@trpc/server": "^10.45.2",
		"arctic": "^1.5.0",
		"lucia": "^3.2.0",
		"lucide-svelte": "^0.376.1",
		"mode-watcher": "^0.3.0",
		"oslo": "^1.2.0",
		"trpc-svelte-query": "^2.1.0",
		"zod": "^3.23.4"
	}
}
